#include <iostream>
#include<string.h>


using namespace std;

int main()
{
    int i,j,len=0;
    
    string s,s1;
   std::getline(std::cin,s);
    for(i=0;s[i]!='\0';i++)
    {
        len++;
    }
    cout<<"length is"<<len<<endl;
    cout<<"the substrings are"<<endl;
    for(i=0;i<len;i++)
    {
        for(j=1;j<len-i;j++)
        {
            s1=s.substr(i,j);
            cout<<s1<<endl;
        }
    }

    return 0;
}
