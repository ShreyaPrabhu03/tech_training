#include <iostream>
#include<ctype.h>


using namespace std;

int main()
{
   char str[100];
   gets(str);
   int i;
   for(i=0;str[i]!='\0';i++)

   {
       if(i==0)
       {
         str[i] = (toupper(str[i]));
       }
       if(isspace(str[i]))
       {
          str[i+1]=(toupper(str[i+1]));
       }

   }
   cout<<str;  
   return 0;
}
