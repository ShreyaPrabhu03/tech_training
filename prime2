#include <stdio.h>
#include <stdlib.h>
#include<math.h>

int isprime(int n)
{

        int i;
    for(i=2;i<=sqrt(n);i++)
    {


        if(n%i==0)

        return 1;

        }
return 0;
}

int main()
{int x,var;
scanf("%d",&x);
var=isprime(x);
printf("%d",var);
if(var)
    printf("it is not prime");

else
    printf("its  prime");
return 0;
}
