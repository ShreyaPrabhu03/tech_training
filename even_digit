#include <stdio.h>
#include <stdlib.h>

int main()
{
    char s[100];
    int i,j;
    int n;
    scanf("%d",&n);
    for(i=0;s[i]!='\0';i++)
    {
        if(isdigit(n))
        {
            n=s[i]-'0';

        for(j=i+1;s[j]!='\0';j++)
        {
            if(isdigit(s[j])
               {


                n = n * 10 + (s[j] - '0');
               }
               else
               {
               i=j-1;
               break;
            }

        }

      if(n%2==0)
      {
          printf("even");
      }

    }






    return 0;
}
