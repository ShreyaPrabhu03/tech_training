#include <stdio.h>
#include <ctype.h> //

int main() {
    char str[100];
    int i, consonantCount = 0;

    printf("Enter a string (no spaces): ");
    scanf("%s", str);


    for(i = 0; str[i] != '\0'; i++) {
        char ch = tolower(str[i]);
        if(isspace(str[i])||isdigit(str[i]))
        continue;

        if ((ch >= 'a' && ch <= 'z') && !(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u')) {
            consonantCount++;
        }
    }

    printf("Number of consonants: %d\n", consonantCount);

    return 0;
}
